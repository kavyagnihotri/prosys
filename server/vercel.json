{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "headers": [
      {
        "source": "/file/upload",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "prosys-client.vercel.app"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "POST"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type"
          }
        ]
      }
    ]
}